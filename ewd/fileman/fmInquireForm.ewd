<ext4:fragment onBeforeRender="menuSelected^xwFilemanInquire">
  <ext4:formPanel id="fileSelectForm" border="false">
    <ext4:fieldSet id="fileLookup" title="<?= #menuSelected ?>" margin="10 10 0 10" >
      <ext4:comboBox fieldLabel="Output from what file" labelWidth="140" name="fileSelect" value="*" />
    </ext4:fieldSet>

    <ext4:submitbutton text="Select" margin="0 10 0 0" nextPage="fmRecordSelect"addTo="westPanel" replacePreviousPage="true" /> 
  </ext4:formPanel>
</ext4:fragment>
